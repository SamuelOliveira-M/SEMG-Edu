{
	"version": 2,
	"devCommand": "npm run dev",
	"builds": [
		{
				  	"src": "public/server.js",
					"use": "@vercel/node",
					"config": { "includeFiles": ["**/*"] }
		}
	],
		"routes": [
						{
									"src": "/(.*)",
									"dest": "public/src/server.js",
									"methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
									"headers": {"Access-Control-Allow-Origin": "*"}
						}
		]
}